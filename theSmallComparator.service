[Unit]
Description=theSmallComparator Web Interface
After=network.target

[Service]
# User/Group will be set by install script
User=YOUR_USER
Group=YOUR_GROUP
WorkingDirectory=YOUR_INSTALL_DIR
ExecStart=YOUR_INSTALL_DIR/theSmallComparator
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target